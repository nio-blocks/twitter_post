{
  "nio/TwitterFavorite": {
    "version": "1.0.0",
    "description": "Favorite tweets on Twitter.",
    "categories": [
      "Social Media"
    ],
    "properties": {
      "creds": {
        "title": "Credentials",
        "type": "ObjectType",
        "description": "Twitter api app credentials.",
        "default": {
          "app_secret": "[[TWITTER_APP_SECRET]]",
          "consumer_key": "[[TWITTER_CONSUMER_KEY]]",
          "oauth_token": "[[TWITTER_ACCESS_TOKEN]]",
          "oauth_token_secret": "[[TWITTER_ACCESS_TOKEN_SECRET]]"
        }
      },
      "id": {
        "title": "Tweet ID",
        "type": "Type",
        "description": "Identifier for tweet to favorite.",
        "default": "{{$id}}"
      }
    },
    "inputs": {
      "default": {
        "description": "Any list of signals."
      }
    },
    "outputs": {},
    "commands": {}
  },
